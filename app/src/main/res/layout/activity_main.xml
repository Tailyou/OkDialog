<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.hengda.frame.hddialog.MainActivity">

    <Button
        android:id="@+id/prgDlgTAnim"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Progress Dlg Tween Anim"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/prgDlgFAnim"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Progress Dlg Frame Anim"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/bldDlgSglBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Builder Dlg Single Button"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/bldDlgNormal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Builder Dlg Two Button"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/bldDlgCustView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Builder Dlg Custom View"
        android:textAllCaps="false" />

</LinearLayout>
